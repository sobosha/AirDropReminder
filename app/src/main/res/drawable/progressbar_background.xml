        <corners android:radius="@dimen/_15sdp"/>
        <gradient android:startColor="#dcdbd7"
            android:endColor="#dcdbd7" />
        <padding android:left="@dimen/_2sdp" android:top="@dimen/_2sdp"
            android:bottom="@dimen/_2sdp" android:right="@dimen/_2sdp"/>
    </shape>
</item>

</layer-list>